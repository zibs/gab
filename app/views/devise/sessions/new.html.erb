<section class="centerer login-page row">

  <h2 class = "login-header">Log in</h2>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name),html: { autocomplete: "off" }) do |f| %>

      <div class="form-group">
      <%# f.label :email %>
      <br>
      <%= f.email_field :email, placeholder: "email", class: "form-control form-overide" %>
    </div>

    <div class="form-group">
      <%# f.label :password %>
      <br>
      <%= f.password_field :password, autocomplete: "off", placeholder: "password", class: "form-control form-overide" %>
    </div>

    <% if devise_mapping.rememberable? -%>
      <div class="form-group rememberable-field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end -%>

    <div class="actions form-group">
      <%= f.submit "Log in", class: "btn btn-lg btn-primary btn-block" %>
    </div>
  <% end %>
  <%= render "devise/shared/links" %>
</section>
